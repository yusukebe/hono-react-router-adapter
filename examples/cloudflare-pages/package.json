    "isbot": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240903.0",
    "@hono/vite-dev-server": "^0.16.0",
    "@playwright/test": "^1.47.0",
    "@remix-run/dev": "^2.11.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "playwright": "^1.47.0",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
